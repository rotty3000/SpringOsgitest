<?xml version="1.0" encoding="UTF-8"?>
<supportAdmin xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
              xsi:noNamespaceSchemaLocation="repository://schemas/supportAdmin-config.xsd">

    <links>
        <link name="Management Console">
            <url>$domain/Admin</url>
            <newWindow>true</newWindow>
        </link>
        <link name="Browser Policy">
            <url>$contextPath/c/document_library/get_file?uuid=9b5a4af3-504c-4881-abd5-c6813095ef3d&amp;groupId=84440
            </url>
            <newWindow>true</newWindow>
        </link>
        <link name="IFS Support Knowledgebase">
            <url>https://my.digitalinsight.com/</url>
            <newWindow>true</newWindow>
        </link>
        <link name="E-channel Terms of Service">
            <url>$domain/bridge/disclosures/ib/disclose.html</url>
            <newWindow>true</newWindow>
        </link>
    </links>

    <products>
        <product appCode="QOL" name="Finance Works">
            <url/>
            <tooltip>true</tooltip>
            <statusMapping defaultText="Eligible">
                <status code="ACTIVE">Opted In</status>
                <status code="ELIGIBLE">Eligible</status>
                <status code="DISABLED">Disabled</status>
                <status code="INELIGIBLE">Ineligible</status>
                <status code="INELIGILBLE">Ineligible</status>    <!-- Typo is present in some older db records -->
            </statusMapping>
        </product>
        <product appCode="ANDRD" name="Android Mobile Banking">
            <url/>
            <tooltip>false</tooltip>
            <statusMapping defaultText="Eligible" hasEntryText="Opted In"/>
        </product>
        <product appCode="IPHONEBANKING" name="iPhone Mobile Banking">
            <url/>
            <tooltip>false</tooltip>
            <statusMapping defaultText="Eligible" hasEntryText="Opted In"/>
        </product>
        <product appCode="IPAD" name="iPad Mobile Banking">
            <url/>
            <tooltip>false</tooltip>
            <statusMapping defaultText="Eligible" hasEntryText="Opted In"/>
        </product>
        <product appCode="TTO" name="TurboTax Online Banking">
            <url/>
            <tooltip>false</tooltip>
            <statusMapping defaultText="Eligible">
                <status code="ACTIVE">In progress</status>
                <status code="COMPLETE">Complete</status>
                <status code="ELIGIBLE">Eligible</status>
                <status code="INCOMPLETE">Not Completed, Last Tax Year</status>
                <status code="PRIOR YEAR">Complete, Last Tax Year</status>
            </statusMapping>
        </product>
        <product appCode="SBFW" name="Small Business Finance Works">
            <url>$contextPath/group/fi-home/usp-dashboard</url>
            <tooltip>false</tooltip>
            <statusMapping defaultText="Eligible">
                <status code="ACTIVE">Opted In</status>
                <status code="DISABLED">Disabled</status>
                <status code="ELIGIBLE">Eligible</status>
                <status code="INELIGIBLE">Ineligible</status>
            </statusMapping>
        </product>
        <product appCode="onlineStatement" name="Online Statements">
            <url/>
            <tooltip>true</tooltip>
            <statusMapping defaultText="Eligible">
                <status code="ELIGIBLE">Eligible</status>
                <status code="OPTED IN">Opted In</status>
            </statusMapping>
        </product>
        <product appCode="BPCF" name="Online Bill Pay">
            <url/>
            <tooltip>false</tooltip>
            <statusMapping defaultText="Eligible">
                <status code="ACTIVE">Opted In</status>
            </statusMapping>
        </product>
        <product appCode="BPMV" name="Online Bill Pay">
            <url/>
            <tooltip>false</tooltip>
            <statusMapping defaultText="Eligible">
                <status code="ACTIVE">Opted In</status>
            </statusMapping>
        </product>
        <product appCode="PR" name="Purchase Rewards">
            <url>/connect/group/purchase-rewards/dashboard#Details?supportAdmin=true</url>
            <tooltip>false</tooltip>
            <statusMapping defaultText="Eligible">
                <status code="ENABLED">Enabled</status>
                <status code="DISABLED">Disabled</status>
                <status code="OPTED OUT">Opted Out</status>
            </statusMapping>
        </product>
    </products>
</supportAdmin>
